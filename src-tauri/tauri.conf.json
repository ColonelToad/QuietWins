{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-app",
  "version": "0.1.0",
  "identifier": "com.legot.tauri-app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Quiet Wins",
        "width": 800,
        "height": 600,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon-warm-16x16.png",
      "icons/icon-warm-32x32.png",
      "icons/icon-warm-128x128.png",
      "icons/icon-warm-256x256.png",
      "icons/icon-warm-512x512.png",
      "icons/icon-warm-1024x1024.png",
      "icons/icon-warm.icns",
      "icons/icon-warm.ico"
    ],
    "resources": [
      "resources",
      "icons/icon-warm-16x16.png"
    ]
  }
}
